{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "/login.html",
      "dest": "/public/login.html"
    },
    {
      "src": "/index.html",
      "dest": "/public/index.html"
    },
    {
      "src": "/style.css",
      "dest": "/public/style.css"
    },
    {
      "src": "/client.js",
      "dest": "/public/client.js"
    },
    {
      "src": "/socket.io/socket.io.js",
      "dest": "/public/socket.io/socket.io.js"
    },
    {
      "src": "/",
      "dest": "/login.html"
    },
    {
      "src": "/login",
      "dest": "server.js"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "server.js"
    }
  ]
}